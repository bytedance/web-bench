{
  "extends": "@parcel/config-default",
  "reporters": [
    "...",
    "./plugins/parcel-reporter-sourcemap-mover",
    "./plugins/parcel-reporter-licenses"
  ],
  "transformers": {
    "*.{js,mjs,jsx,cjs,ts,tsx}": [
      "./plugins/parcel-remove-console-log",
      "..."
    ],
    "*.md": ["./plugins/parcel-transformer-markdown"]
  },
  "resolvers": ["./plugins/parcel-resolver-markdown-i18n", "./plugins/parcel-virtual-files.js", "..."],
  "optimizers": {
    "*.{jpg,jpeg,png,gif,svg}": ["./plugins/parcel-optimizer-imagemin", "..."]
  }
}